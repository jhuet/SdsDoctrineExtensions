{
    "name": "superdweebie/doctrine-extensions",
    "description": "Provides php 5.4 extension traits for Doctrine mongo",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "doctrine"
    ],
    "homepage": "http://github.com/superdweebie/doctrineExtensions",
    "authors": [
        {
            "name": "Tim Roediger",
            "email": "superdweebie@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "superdweebie/common":               "~0.3",
        "superdweebie/validator":            "~0.3",
        "doctrine/mongodb-odm":              "1.*",
        "zendframework/zend-json":           "~2.1",
        "zendframework/zend-crypt":          "~2.1",
        "zendframework/zend-servicemanager": "~2.1",
        "zendframework/zend-stdlib":         "~2.1"
    },
    "autoload": {
        "psr-0": {
            "Sds\\DoctrineExtensions": "lib/"
        }
    },
    "config": {
        "github-protocols": ["http", "https", "git"]
    }
}